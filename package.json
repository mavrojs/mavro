{
  "name": "mavro",
  "version": "0.0.7",
  "description": "A minimal, fast, and scalable Node.js framework.",
  "main": "dist/index.js",
  "keywords": [
    "nodejs",
    "framework",
    "koa",
    "typescript",
    "mvc"
  ],
  "files": [
    "dist"
  ],
  "contributors": [
    "Othmane Nejdi <github@iietmoon>"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --exec ts-node src/index.ts",
    "start": "ts-node dist/index.js",
    "test": "jest",
    "prepare": "husky",
    "release": "standard-version",
    "release:alpha": "standard-version --prerelease alpha",
    "release:beta": "standard-version --prerelease beta",
    "release:rc": "standard-version --prerelease rc"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mavrojs/mavro.git"
  },
  "packageManager": "npm@10.8.1",
  "engines": {
    "node": ">=v18.18.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "@types/node": "^22.5.5",
    "eslint": "^9.11.1",
    "esm": "^3.2.25",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "standard-version": "^9.5.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "colors": "^1.4.0",
    "dotenv": "^16.4.5"
  }
}
